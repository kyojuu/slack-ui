{
  "expo": {
    "name": "Expo Slack",
    "description": "Slack UI Clone with Expo",
    "scheme": "slackclone",
    "slug": "slackclone",
    "privacy": "public",
    "version": "0.4.0",
    "platforms": ["android", "ios", "web"],
    "githubUrl": "https://github.com/calebnance/expo-slack",
    "orientation": "portrait",
    "primaryColor": "#ffffff",
    "icon": "src/assets/icon.png",
    "splash": {
      "backgroundColor": "#ffffff",
      "image": "src/assets/splash.png",
      "resizeMode": "contain"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "android": {
      "versionCode": 40
    },
    "ios": {
      "buildNumber": "0.4.0",
      "supportsTablet": true
    }
  }
}
